// Test: Compound assignment on 1D arrays
// Covers all compound operators: +<-, -<-, *<-, /<-, %<-, &<-, |<-, ^<-, <<<-, >><-
u32 data[10];

i32 main() {
    // Initialize array elements
    data[0] <- 100;
    data[1] <- 50;
    data[2] <- 10;
    data[3] <- 100;
    data[4] <- 17;
    data[5] <- 0b11110000;
    data[6] <- 0b00001111;
    data[7] <- 0b10101010;
    data[8] <- 8;
    data[9] <- 128;

   
// Test all compound assignment operators on array elements
    data[0] +<- 25; // 100 + 25 = 125
    data[1] -<- 15; // 50 - 15 = 35
    data[2] *<- 7; // 10 * 7 = 70
    data[3] /<- 4; // 100 / 4 = 25
    data[4] %<- 5; // 17 % 5 = 2
    data[5] &<- 0b11001100; // 0b11110000 & 0b11001100 = 0b11000000 = 192
    data[6] |<- 0b11110000; // 0b00001111 | 0b11110000 = 0b11111111 = 255
    data[7] ^<- 0b11111111; // 0b10101010 ^ 0b11111111 = 0b01010101 = 85
    data[8] <<<- 2; // 8 << 2 = 32
    data[9] >><- 3; // 128 >> 3 = 16

    // Verify all results
    if (data[0] != 125) {
        return 1;
    }
    if (data[1] != 35) {
        return 2;
    }
    if (data[2] != 70) {
        return 3;
    }
    if (data[3] != 25) {
        return 4;
    }
    if (data[4] != 2) {
        return 5;
    }
    if (data[5] != 192) {
        return 6;
    }
    if (data[6] != 255) {
        return 7;
    }
    if (data[7] != 85) {
        return 8;
    }
    if (data[8] != 32) {
        return 9;
    }
    if (data[9] != 16) {
        return 10;
    }

    return 0;
}
