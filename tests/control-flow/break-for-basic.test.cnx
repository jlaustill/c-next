/* test-execution */ // Tests: break statement in for loop
u32 main() {
    u32 count <- 0;

    for (u32 i <- 0; i < 100; i <- i + 1) {
        count +<- 1;
        if (i = 9) {
            break;
        }
    }

    // Should have counted 0-9 (10 iterations)
    if (count != 10) return 1;

    return 0;
}
