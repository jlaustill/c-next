/* test-execution */ // ADR-013 + ADR-044: Const wrap u16 combination
// Tests: const wrap u16 declaration and read access
const wrap u16 MAX_COUNTER <- 65535;

const wrap u16 START_VALUE <- 1000;

u32 main() {
    // Test 1: Read const wrap into local
    u16 maxCnt <- MAX_COUNTER;
    if (maxCnt != 65535) return 1;

    // Test 2: Read second const wrap
    u16 start <- START_VALUE;
    if (start != 1000) return 2;

    // Test 3: Use const wrap in arithmetic
    u16 diff <- MAX_COUNTER - START_VALUE;
    if (diff != 64535) return 3;

    return 0;
}
