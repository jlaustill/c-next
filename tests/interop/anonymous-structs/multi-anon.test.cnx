// test-cpp-only
// test-execution
// Test: Multiple anonymous struct fields in one parent struct
#include "anon_types.h"

i32 main() {
    // Test 1: Multiple anonymous struct fields
    DisplayConfig display <- {
        resolution: { width: 1920, height: 1080 },
        settings: { brightness: 80, contrast: 50 }
    };

    if (display.resolution.width != 1920) return 1;
    if (display.resolution.height != 1080) return 2;
    if (display.settings.brightness != 80) return 3;
    if (display.settings.contrast != 50) return 4;

    // Test 2: Partial init of multiple anonymous fields
    DisplayConfig display2 <- {
        resolution: { width: 800, height: 600 }
    };

    if (display2.resolution.width != 800) return 5;
    if (display2.resolution.height != 600) return 6;

    return 0;
}
