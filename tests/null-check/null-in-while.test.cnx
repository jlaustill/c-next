// test-coverage: 28-null-in-while
// Tests: NULL check in while loop condition
#include <stdio.h>

string<64> buffer;

// Read lines until fgets returns NULL (EOF)
void readAllLines() {
    i8* result <- fgets(buffer, buffer.size, stdin);
    while (result != NULL) {
        // Process each line
        printf("Line: %s", buffer);
        result <- fgets(buffer, buffer.size, stdin);
    }
}

// Alternative pattern with = NULL check
void readUntilEmpty() {
    i8* result <- fgets(buffer, buffer.size, stdin);
    while (result = NULL) {
        // This pattern exits on first NULL
        printf("Got NULL\n");
        result <- fgets(buffer, buffer.size, stdin);
    }
}

void main() {
    readAllLines();
}
