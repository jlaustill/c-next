// test-execution
// Issue #107: Negative Case Labels - i32 Type
// Tests: negative literals with i32 switch expressions
i32 result;

// Test i32 with various negative values
void testI32Values(i32 val) {
    switch (val) {
        case -2147483648 {
            result <- 1; // INT32_MIN
        }
        case -2147483647 {
            result <- 2; // INT32_MIN + 1
        }
        case -1000000 {
            result <- 3; // Large negative
        }
        case -32769 {
            result <- 4; // Just beyond i16 min
        }
        case -32768 {
            result <- 5; // i16 min boundary
        }
        case -1 {
            result <- 6;
        }
        case 0 {
            result <- 7;
        }
        case 32767 {
            result <- 8; // i16 max boundary
        }
        case 32768 {
            result <- 9; // Just beyond i16 max
        }
        case 1000000 {
            result <- 10; // Large positive
        }
        case 2147483646 {
            result <- 11; // INT32_MAX - 1
        }
        case 2147483647 {
            result <- 12; // INT32_MAX
        }
    default {
            result <- 99;
        }
    }
}

// Test negative hex with i32 values
void testI32Hex(i32 val) {
    switch (val) {
        case -0x80000000 {
            result <- 20; // INT32_MIN
        }
        case -0x10000 {
            result <- 21; // -65536
        }
        case -0x8000 {
            result <- 22; // -32768 (i16 min)
        }
        case -0xFF {
            result <- 23; // -255
        }
        case 0x00 {
            result <- 24;
        }
        case 0x7FFFFFFF {
            result <- 25; // INT32_MAX
        }
    default {
            result <- 99;
        }
    }
}

u32 main() {
   
// Test i32 values
    i32 minVal <- -2147483648;
    i32 minPlus1 <- -2147483647;
    i32 negMillion <- -1000000;
    i32 neg32769 <- -32769;
    i32 neg32768 <- -32768;
    i32 negOne <- -1;
    i32 zero <- 0;
    i32 pos32767 <- 32767;
    i32 pos32768 <- 32768;
    i32 posMillion <- 1000000;
    i32 maxMinus1 <- 2147483646;
    i32 maxVal <- 2147483647;
    i32 other <- 12345;

    testI32Values(minVal);
    if (result != 1) return 1;

    testI32Values(minPlus1);
    if (result != 2) return 2;

    testI32Values(negMillion);
    if (result != 3) return 3;

    testI32Values(neg32769);
    if (result != 4) return 4;

    testI32Values(neg32768);
    if (result != 5) return 5;

    testI32Values(negOne);
    if (result != 6) return 6;

    testI32Values(zero);
    if (result != 7) return 7;

    testI32Values(pos32767);
    if (result != 8) return 8;

    testI32Values(pos32768);
    if (result != 9) return 9;

    testI32Values(posMillion);
    if (result != 10) return 10;

    testI32Values(maxMinus1);
    if (result != 11) return 11;

    testI32Values(maxVal);
    if (result != 12) return 12;

    testI32Values(other);
    if (result != 99) return 13;

   
// Test hex literals
    testI32Hex(minVal);
    if (result != 20) return 14;

    i32 neg65536 <- -65536;
    testI32Hex(neg65536);
    if (result != 21) return 15;

    testI32Hex(neg32768);
    if (result != 22) return 16;

    i32 neg255 <- -255;
    testI32Hex(neg255);
    if (result != 23) return 17;

    testI32Hex(zero);
    if (result != 24) return 18;

    testI32Hex(maxVal);
    if (result != 25) return 19;

    return 0;
}
