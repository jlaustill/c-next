// test-execution
// Tests: Scope method returning global enum

enum EGlobal {
    A <- 0,
    B <- 1
}

scope Test {
    public global.EGlobal getGlobalEnum() {
        return global.EGlobal.A;
    }
}

u32 main() {
    // Scope method returning global enum - does this work?
    EGlobal fromScope <- Test.getGlobalEnum();
    if (fromScope != EGlobal.A) return 1;
    return 0;
}
