// test-execution
// Tests: f32 to i16 cast should clamp values to -32768 to 32767 range

u32 main() {
    f32 over <- 40000.0;
    f32 under <- -40000.0;
    f32 in_range <- 10000.0;

    i16 result <- (i16)over;
    if (result != 32767) return 1;

    result <- (i16)under;
    if (result != -32768) return 2;

    result <- (i16)in_range;
    if (result != 10000) return 3;

    return 0;
}
