// test-execution
// Tests: f32 to u16 cast should clamp values to 0-65535 range

u32 main() {
    f32 over <- 70000.0;
    f32 negative <- -100.0;
    f32 in_range <- 30000.0;

    u16 result <- (u16)over;
    if (result != 65535) return 1;

    result <- (u16)negative;
    if (result != 0) return 2;

    result <- (u16)in_range;
    if (result != 30000) return 3;

    return 0;
}
