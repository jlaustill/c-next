// test-execution
// Tests: continue statement in for loop
// Validates: skipping iterations with continue in for loops
u32 main() {
   
// Test 1: Skip even numbers, sum odd numbers
    u32 oddSum <- 0;
    for (u32 i <- 0; i < 10; i +<- 1) {
        if (i % 2 = 0) {
            continue;
        }
        oddSum +<- i;
    }

    // oddSum = 1 + 3 + 5 + 7 + 9 = 25
    if (oddSum != 25) return 1;

   
// Test 2: Skip specific value
    u32 sumExcept5 <- 0;
    for (u32 j <- 0; j < 10; j +<- 1) {
        if (j = 5) {
            continue;
        }
        sumExcept5 +<- j;
    }

    // sum = 0+1+2+3+4+6+7+8+9 = 40
    if (sumExcept5 != 40) return 2;

   
// Test 3: Multiple continue conditions
    u32 filtered <- 0;
    for (u32 k <- 0; k < 20; k +<- 1) {
        if (k % 2 = 0) {
            continue; // skip even
        }
        if (k % 5 = 0) {
            continue; // skip multiples of 5
        }
        filtered +<- k;
    }

    // Odd numbers not divisible by 5: 1,3,7,9,11,13,17,19
    // = 1+3+7+9+11+13+17+19 = 80
    if (filtered != 80) return 3;

   
// Test 4: Continue with counter accumulation
    u32 count <- 0;
    u32 iterations <- 0;
    for (u32 m <- 0; m < 10; m +<- 1) {
        iterations +<- 1;
        if (m < 5) {
            continue;
        }
        count +<- 1;
    }
    if (iterations != 10) return 4;

    // count should be 5 (for m = 5,6,7,8,9)
    if (count != 5) return 5;

   
// Test 5: Continue on first iteration
    u32 skipFirst <- 0;
    for (u32 p <- 0; p < 5; p +<- 1) {
        if (p = 0) {
            continue;
        }
        skipFirst +<- p;
    }

    // sum = 1+2+3+4 = 10
    if (skipFirst != 10) return 6;

   
// Test 6: Continue on last iteration
    u32 skipLast <- 0;
    for (u32 q <- 0; q < 5; q +<- 1) {
        if (q = 4) {
            continue;
        }
        skipLast +<- q;
    }

    // sum = 0+1+2+3 = 6
    if (skipLast != 6) return 7;

   
// Test 7: Complex condition with continue
    u32 complexSum <- 0;
    for (u32 r <- 1; r <= 20; r +<- 1) {
        if (r % 2 = 0 && r % 3 = 0) {
            continue; // Skip numbers divisible by both 2 and 3 (i.e., 6, 12, 18)
        }
        complexSum +<- r;
    }

    // 1+2+3+4+5+7+8+9+10+11+13+14+15+16+17+19+20 = 174
    // (skipped 6, 12, 18)
    if (complexSum != 174) return 8;

    return 0;
}
