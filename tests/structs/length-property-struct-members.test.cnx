// test-execution
// Regression test: .length property on struct member access
// Verifies cfg.magic.length correctly returns 32 (was returning 0)
struct Config {
    u32 magic;
    u16 version;
    u8 flags;
}

i32 main() {
    Config cfg;
    cfg.magic <- 0x12345678;
    cfg.version <- 1;
    cfg.flags <- 0xFF;

   
// Test .length on struct members
    if (cfg.magic.length != 32) {
        return 1;
    }

    if (cfg.version.length != 16) {
        return 1;
    }

    if (cfg.flags.length != 8) {
        return 1;
    }

    return 0;
}
