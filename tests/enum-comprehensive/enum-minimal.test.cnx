// test-execution
// Tests: Minimal enum test

scope Motor {
    public enum EMode {
        OFF <- 0,
        LOW <- 1
    }

    private this.EMode mode <- this.EMode.OFF;

    public this.EMode getMode() {
        return this.mode;
    }
}

u32 main() {
    // Test: calling scope method that returns enum
    Motor.EMode motorMode <- Motor.getMode();
    if (motorMode != Motor.EMode.OFF) return 1;
    return 0;
}
