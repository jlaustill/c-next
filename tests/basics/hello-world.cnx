#include <stdio.h>

string<50> userName;

u32 main(string args[]) {
    printf("What is your name in 50 characters or less? ");

    fgets(userName, userName.capacity, stdin);

    if (userName.length > 0 && userName[userName.length - 1] = '\n') {
        userName[userName.length - 1] <- '\0';
    }

    printf("Hello from %s %s!\n", args[0], userName);

    return 0;
}