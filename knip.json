{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/lib/parseWithSymbols.ts", "scripts/*.ts"],
  "project": ["src/**/*.ts", "scripts/**/*.ts"],
  "ignore": [
    "prettier-plugin/**",
    "vscode-extension/**",
    "tests/**",
    "src/parser/grammar/**",
    "src/codegen/assignment/AssignmentContextBuilder.ts",
    "src/transpiler/output/codegen/generators/expressions/PostfixExpressionGenerator.ts",
    "src/transpiler/output/codegen/helpers/CppModeHelper.ts",
    "src/transpiler/output/codegen/analysis/StringLengthCounter.ts"
  ],
  "ignoreBinaries": ["jscpd", "jq", "dot", "madge", "ts-prune"],
  "ignoreDependencies": ["madge", "ts-prune"]
}
