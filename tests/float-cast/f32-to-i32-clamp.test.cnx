// test-execution
// Tests: f32 to i32 cast should clamp values to -2147483648 to 2147483647 range

u32 main() {
    f32 over <- 3000000000.0;   // > INT32_MAX
    f32 under <- -3000000000.0; // < INT32_MIN
    f32 in_range <- 1000000.0;

    i32 result <- (i32)over;
    if (result != 2147483647) return 1;

    result <- (i32)under;
    if (result != -2147483648) return 2;

    result <- (i32)in_range;
    if (result != 1000000) return 3;

    return 0;
}
