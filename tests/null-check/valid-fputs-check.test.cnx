// ADR-046: Valid EOF check pattern for fputs using c_ prefix
#include <stdio.h>

string<32> message <- "Hello, World!";

void writeOutput() {
    // fputs returns EOF (not NULL) on error, but pattern is similar
    i32 c_result <- fputs(message, stdout);
    if (c_result != EOF) {
        // Write succeeded
    }
}
