/* test-execution */ // Coverage: Section 7.3 - Nested do-while loops
// Tests: do-while loops nested inside other do-while loops
u32 main() {
    u32 i <- 0;
    u32 j <- 0;
    u32 count <- 0;

    // Test 1: Simple nested do-while
    i <- 0;
    do {
        j <- 0;
        do {
            count +<- 1;
            j +<- 1;
        } while (j < 3);
        i +<- 1;
    } while (i < 3);
    if (count != 9) return 1;

    // Test 2: Deeply nested (3 levels)
    u32 k <- 0;
    count <- 0;
    i <- 0;
    do {
        j <- 0;
        do {
            k <- 0;
            do {
                count +<- 1;
                k +<- 1;
            } while (k < 2);
            j +<- 1;
        } while (j < 2);
        i +<- 1;
    } while (i < 2);
    if (count != 8) return 2;

    // Test 3: Nested with different conditions
    count <- 0;
    i <- 0;
    do {
        j <- 0;
        do {
            count +<- 1;
            j +<- 1;
        } while (j < i + 1);
        i +<- 1;
    } while (i < 4);

    // 1 + 2 + 3 + 4 = 10
    if (count != 10) return 3;

    return 0;
}
