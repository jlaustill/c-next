// Issue #471: Cross-file enum resolution in case labels
// Tests that enum values from included files get resolved correctly
#include "switch-enum-cross-file-types.cnx"

u32 classify(EStatus status) {
    switch (status) {
        case PENDING {
            return 1;
        }
        case ACTIVE {
            return 2;
        }
        case COMPLETED {
            return 3;
        }
    }
    return 0;
}
