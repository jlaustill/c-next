// test-execution
// Tests: u8 comparison operations (<, >, <=, >=, =, !=)
// Coverage: Section 3.3-3.6 for u8 type
u32 main() {
    u8 a <- 100;
    u8 b <- 50;
    u8 c <- 100;
    u8 zero <- 0;
    u8 max_val <- 255;

    // === Equality (=) ===
    if ((a = c) != true) return 1;
    if ((a = b) != false) return 2;
    if ((zero = 0) != true) return 3;

    // === Not Equal (!=) ===
    if ((a != b) != true) return 4;
    if ((a != c) != false) return 5;
    if ((zero != 1) != true) return 6;

    // === Less Than (<) ===
    if ((b < a) != true) return 7;      // 50 < 100
    if ((a < b) != false) return 8;     // 100 < 50
    if ((a < a) != false) return 9;     // equal values
    if ((zero < 1) != true) return 10;  // 0 < 1
    if ((zero < max_val) != true) return 11;

    // === Greater Than (>) ===
    if ((a > b) != true) return 12;     // 100 > 50
    if ((b > a) != false) return 13;    // 50 > 100
    if ((a > a) != false) return 14;    // equal values
    if ((max_val > zero) != true) return 15;
    if ((1 > zero) != true) return 16;

    // === Less Than or Equal (<=) ===
    if ((b <= a) != true) return 17;    // 50 <= 100
    if ((a <= c) != true) return 18;    // 100 <= 100 (equal)
    if ((a <= b) != false) return 19;   // 100 <= 50
    if ((zero <= zero) != true) return 20;
    if ((zero <= max_val) != true) return 21;

    // === Greater Than or Equal (>=) ===
    if ((a >= b) != true) return 22;    // 100 >= 50
    if ((a >= c) != true) return 23;    // 100 >= 100 (equal)
    if ((b >= a) != false) return 24;   // 50 >= 100
    if ((max_val >= zero) != true) return 25;
    if ((max_val >= max_val) != true) return 26;

    // === Edge cases with max value ===
    u8 almost_max <- 254;
    if ((max_val > almost_max) != true) return 27;
    if ((almost_max < max_val) != true) return 28;

    // === Literal comparisons ===
    if ((50 < a) != true) return 29;    // literal < variable
    if ((100 > b) != true) return 30;   // literal > variable

    return 0;
}
