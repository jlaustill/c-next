// Tests: Qualified enum member used as array dimension
// EColor.COUNT should generate EColor_COUNT in both code and header

enum EColor {
    RED,
    GREEN,
    BLUE,
    COUNT
}

const u8[EColor.COUNT] DATA <- [10, 20, 30];

scope Test {
    public u8 get(u8 idx) {
        return global.DATA[idx];
    }
}
