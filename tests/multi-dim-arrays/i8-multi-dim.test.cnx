/* test-execution */
// Tests: multi-dimensional i8 arrays (2D and 3D)
// Validates array indexing, length properties, and nested iteration
// Typical use case: temperature sensors, small signed offsets, delta values

i8 matrix2d[4][8];
i8 cube3d[2][3][4];

u32 main() {
    // 2D array: Write operations with positive and negative values
    matrix2d[0][0] <- -128;
    matrix2d[0][7] <- 127;
    matrix2d[1][3] <- -50;
    matrix2d[3][7] <- 100;

    // 2D array: Read and verify
    if (matrix2d[0][0] != -128) return 1;
    if (matrix2d[0][7] != 127) return 2;
    if (matrix2d[1][3] != -50) return 3;
    if (matrix2d[3][7] != 100) return 4;

    // 3D array: Write operations
    cube3d[0][0][0] <- -100;
    cube3d[0][0][3] <- -25;
    cube3d[0][2][3] <- 0;
    cube3d[1][0][0] <- 50;
    cube3d[1][2][3] <- 127;

    // 3D array: Read and verify
    if (cube3d[0][0][0] != -100) return 5;
    if (cube3d[0][0][3] != -25) return 6;
    if (cube3d[0][2][3] != 0) return 7;
    if (cube3d[1][0][0] != 50) return 8;
    if (cube3d[1][2][3] != 127) return 9;

    // Test .length property at each dimension
    // 2D array dimensions
    if (matrix2d.length != 4) return 10;       // First dimension
    if (matrix2d[0].length != 8) return 11;    // Second dimension
    if (matrix2d[0][0].length != 8) return 12; // Element bit width

    // 3D array dimensions
    if (cube3d.length != 2) return 13;          // First dimension
    if (cube3d[0].length != 3) return 14;       // Second dimension
    if (cube3d[0][0].length != 4) return 15;    // Third dimension
    if (cube3d[0][0][0].length != 8) return 16; // Element bit width

    // Nested loop iteration - populate 2D array with small values
    for (u32 i <- 0; i < 4; i +<- 1) {
        for (u32 j <- 0; j < 8; j +<- 1) {
            matrix2d[i][j] <- i * 5 + j;
        }
    }

    // Verify populated values
    if (matrix2d[0][0] != 0) return 17;
    if (matrix2d[0][7] != 7) return 18;
    if (matrix2d[1][3] != 8) return 19;
    if (matrix2d[3][7] != 22) return 20;

    // Nested loop iteration - populate and sum 3D array
    i16 sum <- 0;  // Use i16 for sum accumulator
    for (u32 i <- 0; i < 2; i +<- 1) {
        for (u32 j <- 0; j < 3; j +<- 1) {
            for (u32 k <- 0; k < 4; k +<- 1) {
                cube3d[i][j][k] <- i * 5 + j * 2 + k;
                sum +<- cube3d[i][j][k];
            }
        }
    }

    // Verify sum: 6+14+22+26+34+42 = 144
    if (sum != 144) return 21;

    // Test modification after initialization
    matrix2d[2][4] <- -128;
    if (matrix2d[2][4] != -128) return 22;

    // Test boundary access
    matrix2d[matrix2d.length - 1][matrix2d[0].length - 1] <- -1;
    if (matrix2d[3][7] != -1) return 23;

    // Test i8 specific edge cases
    i8 testArray[4][2];
    // Initialize all elements first
    for (u32 ti <- 0; ti < 4; ti +<- 1) {
        for (u32 tj <- 0; tj < 2; tj +<- 1) {
            testArray[ti][tj] <- 0;
        }
    }
    testArray[0][0] <- -128;  // Min value
    testArray[0][1] <- 127;   // Max value
    testArray[1][0] <- 0;     // Zero
    testArray[1][1] <- -1;    // Negative
    testArray[2][0] <- 1;     // Positive
    testArray[3][1] <- -50;   // Mid negative

    if (testArray[0][0] != -128) return 24;
    if (testArray[0][1] != 127) return 25;
    if (testArray[1][0] != 0) return 26;
    if (testArray[1][1] != -1) return 27;
    if (testArray[2][0] != 1) return 28;
    if (testArray[3][1] != -50) return 29;

    // Test 2D array with temperature sensor pattern (in Celsius)
    i8 temps[3][3];
    // Initialize all elements first
    for (u32 tei <- 0; tei < 3; tei +<- 1) {
        for (u32 tej <- 0; tej < 3; tej +<- 1) {
            temps[tei][tej] <- 0;
        }
    }
    temps[0][0] <- -40;  // Very cold
    temps[0][1] <- -10;  // Cold
    temps[1][0] <- 0;    // Freezing
    temps[1][1] <- 20;   // Room temp
    temps[2][0] <- 37;   // Body temp
    temps[2][1] <- 100;  // Boiling

    if (temps[0][0] != -40) return 30;
    if (temps[0][1] != -10) return 31;
    if (temps[1][0] != 0) return 32;
    if (temps[1][1] != 20) return 33;
    if (temps[2][0] != 37) return 34;
    if (temps[2][1] != 100) return 35;

    // Test negative value calculations
    i8 deltas[2][2];
    // Initialize all elements first
    for (u32 di <- 0; di < 2; di +<- 1) {
        for (u32 dj <- 0; dj < 2; dj +<- 1) {
            deltas[di][dj] <- 0;
        }
    }
    deltas[0][0] <- -10;
    deltas[0][1] <- 5;
    i16 deltaSum <- deltas[0][0] + deltas[0][1];
    if (deltaSum != -5) return 36;

    return 0;
}
