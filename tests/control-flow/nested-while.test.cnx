/* test-execution */
// Coverage: Section 7.2 - Nested while loops
// Tests: while loops nested inside other while loops

u32 main() {
    u32 i <- 0;
    u32 j <- 0;
    u32 count <- 0;

    // Test 1: Simple nested while
    i <- 0;
    while (i < 3) {
        j <- 0;
        while (j < 3) {
            count +<- 1;
            j +<- 1;
        }
        i +<- 1;
    }
    if (count != 9) return 1;

    // Test 2: Deeply nested (3 levels)
    u32 k <- 0;
    count <- 0;
    i <- 0;
    while (i < 2) {
        j <- 0;
        while (j < 2) {
            k <- 0;
            while (k < 2) {
                count +<- 1;
                k +<- 1;
            }
            j +<- 1;
        }
        i +<- 1;
    }
    if (count != 8) return 2;

    // Test 3: Inner loop with different exit condition
    count <- 0;
    i <- 0;
    while (i < 5) {
        j <- 0;
        while (j < i) {
            count +<- 1;
            j +<- 1;
        }
        i +<- 1;
    }
    // 0 + 1 + 2 + 3 + 4 = 10
    if (count != 10) return 3;

    return 0;
}
