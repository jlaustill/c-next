// test-execution
// Tests: f64 to i64 cast should clamp values within range
// Note: f64 can represent values beyond i64 range but with precision loss

u32 main() {
    f64 in_range <- 1000000000000.0;  // 1 trillion
    f64 negative <- -500000000000.0;  // -500 billion

    i64 result <- (i64)in_range;
    if (result != 1000000000000) return 1;

    result <- (i64)negative;
    if (result != -500000000000) return 2;

    return 0;
}
