// test-execution
// Test i64 comparison operations
// Coverage: Section 3.1-3.6 for i64 type
u32 main() {
    i64 a <- 5000000000;
    i64 b <- 3000000000;
    i64 c <- 5000000000; // Equal to a
    i64 d <- -2000000000;
    i64 e <- -5000000000;

    // Equality (=)
    bool eq1 <- a = c; // true
    bool eq2 <- a = b; // false
    bool eq3 <- d = -2000000000; // true (literal)

    // Not Equal (!=)
    bool neq1 <- a != b; // true
    bool neq2 <- a != c; // false
    bool neq3 <- d != 0; // true

    // Less Than (<)
    bool lt1 <- b < a; // true (3000000000 < 5000000000)
    bool lt2 <- a < b; // false
    bool lt3 <- d < 0; // true (negative < 0)
    bool lt4 <- e < d; // true (-5000000000 < -2000000000)

    // Greater Than (>)
    bool gt1 <- a > b; // true (5000000000 > 3000000000)
    bool gt2 <- b > a; // false
    bool gt3 <- 0 > d; // true (0 > negative)
    bool gt4 <- d > e; // true (-2000000000 > -5000000000)

    // Less Than or Equal (<=)
    bool lte1 <- b <= a; // true (3000000000 <= 5000000000)
    bool lte2 <- a <= c; // true (5000000000 <= 5000000000)
    bool lte3 <- a <= b; // false

    // Greater Than or Equal (>=)
    bool gte1 <- a >= b; // true (5000000000 >= 3000000000)
    bool gte2 <- a >= c; // true (5000000000 >= 5000000000)
    bool gte3 <- b >= a; // false

    // Edge cases - max/min
    i64 max_val <- 9223372036854775807;
    i64 min_val <- -9223372036854775808;
    bool max_gt_min <- max_val > min_val; // true
    bool min_lt_max <- min_val < max_val; // true

    // Verify all results
    if (eq1 && !eq2 && eq3) {
        if (neq1 && !neq2 && neq3) {
            if (lt1 && !lt2 && lt3 && lt4) {
                if (gt1 && !gt2 && gt3 && gt4) {
                    if (lte1 && lte2 && !lte3) {
                        if (gte1 && gte2 && !gte3) {
                            if (max_gt_min && min_lt_max) {
                                return 0; // All tests passed
                            }
                        }
                    }
                }
            }
        }
    }
    return 1;
// Test failed
}
