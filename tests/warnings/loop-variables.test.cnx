/* test-no-warnings */
// test-execution
// Tests: Loop variable usage should not trigger unused variable warnings
// Validates: Loop counters and accumulators are properly used
// Coverage: While loops, compound assignment in loops

u32 main() {
    // Test basic while loop counter
    u32 counter <- 0;
    while (counter < 10) {
        counter +<- 1;
    }
    if (counter != 10) return 1;

    // Test accumulator pattern
    u32 sum <- 0;
    u32 i <- 1;
    while (i <= 5) {
        sum +<- i;
        i +<- 1;
    }
    if (sum != 15) return 2;

    // Test decrementing counter
    u32 countdown <- 5;
    u32 iterations <- 0;
    while (countdown > 0) {
        countdown -<- 1;
        iterations +<- 1;
    }
    if (iterations != 5) return 3;

    // Test nested loop counters
    u32 outer <- 0;
    u32 total <- 0;
    while (outer < 3) {
        u32 inner <- 0;
        while (inner < 3) {
            total +<- 1;
            inner +<- 1;
        }
        outer +<- 1;
    }
    if (total != 9) return 4;

    return 0;
}
