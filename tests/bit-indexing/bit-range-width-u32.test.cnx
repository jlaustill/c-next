// test-execution
// Tests: All bit range widths 1-32 on u32 type
// Verifies correct mask generation and bit operations for every width

u32 main() {
    u32 value <- 0;

    // Width 1: mask 0x1
    value <- 0;
    value[0, 1] <- 1;
    if (value != 0x00000001) return 1;

    // Width 2: mask 0x3
    value <- 0;
    value[0, 2] <- 3;
