// test-execution
// Tests: Global function returning enum

enum EState {
    OFF <- 0,
    ON <- 1
}

EState globalState <- EState.OFF;

EState getState() {
    return globalState;
}

u32 main() {
    // Does global function returning enum work?
    EState current <- getState();
    if (current != EState.OFF) return 1;
    return 0;
}
